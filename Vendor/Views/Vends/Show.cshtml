@{
  Layout = "_Layout";
}

@using Vendor.Models;

<h3>Here are all the items in this category:</h3>

<ol>
@foreach (Order order in @Model["orders"])
{
  <li><a href='/vendors/@Model["vendor"].VendId/orders/@order.OrderId'>@order.OrderInfo</a></li>
}
</ol>

<p><a href='/vendors/@Model["vendor"].VendId/orders/new'>Add a new order for this Vendor</a></p>
<p><a href='/vendors'>Return to list of Vendors</a></p>
<p><a href='/'>Return to Home Page</a></p>