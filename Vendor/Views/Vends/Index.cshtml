@{
  Layout = "_Layout";
}

@using Vendor.Models;

<h1 class="smallHead">Your Current Vendors</h1>

@if (@Model.Count == 0)
{
  <h2>You have not added any Vendors yet.</h2>
} 

@foreach (Vend vend in Model)
{
  <h3><a href='/vendors/@vend.VendId'>@vend.VendName</a></h3>
}
<hr>
<p><a href='/'>Back home</a></p>